<path fill="#FCE100" d="M226.1,79.7c-0.4-0.4-1-0.4-1.4,0c-0.1,0.1-0.2,0.3-0.2,0.4c0.6,0.1,1.3,0.3,1.9,0.4
	C226.4,80.2,226.3,79.9,226.1,79.7z"/>
